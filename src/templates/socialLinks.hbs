{{#if gplus}}
	<a href="https://plus.google.com/{{gplus}}" class="gplus" target="_blank" title="Google+ Profile"><i class="icon-gplus"></i></a>
{{/if}}
{{#if github}}
	<a href="https://github.com/{{github}}" class="github" target="_blank" title="GitHub Profile"><i class="icon-github"></i></a>
{{/if}}
{{#if twitter}}
	<a href="https://twitter.com/{{twitter}}" class="twitter" target="_blank" title="Twitter Profile"><i class="icon-twitter"></i></a>
{{/if}}
{{#if youtube}}
	<a href="https://www.youtube.com/c/{{youtube}}" class="youtube" target="_blank" title="YouTube Channel"><i class="icon-youtube"></i></a>
{{/if}}
